<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
             Title="Biceps"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileProject2018.Bicep">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
            <Button Text="Standing Barbell Curl" Clicked="BarbellCurl_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Preacher Curl" Clicked="PreacherCurl_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
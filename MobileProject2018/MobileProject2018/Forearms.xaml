<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
             Title="Forearms"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileProject2018.Forearms">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
            <Button Text="Wrist Curl Behind Back" Clicked="BehindBackWristCurls_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Seated Wrist Curls" Clicked="SeatedWristCurls_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
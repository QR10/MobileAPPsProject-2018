<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
             Title="Back"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileProject2018.Back">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
            <Button Text="Lat Pulldown" Clicked="LatPulldown_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Seated Cable Row" Clicked="SeatedRow_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Tripod Dumbbell Row" Clicked="DumbbellRow_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
<?xml version="1.0" encoding="utf-8" ?>
<ContentPage 
             Title="Saturday"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileProject2018.Week_Saturday">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
            <Button Text="Shoulders" Clicked="Shoulders_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Calves" Clicked="Calves_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Forearms" Clicked="Forearms_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
            <Button Text="Lats" Clicked="Lats_Clicked" FontSize="20" BackgroundColor="#1dabf0" TextColor="White"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>